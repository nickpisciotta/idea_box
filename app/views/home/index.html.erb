

  <form>
    Title:<br>
    <input type="text" name="title" id="idea-title"><br>
    Body:<br>
    <input type="text" name="body" id="idea-body">
  </form></br>
  <div>
    <input class="btn btn-default" id="create-idea" type="button" name="submit" value="Save">
  </div>


<div class="">
  <h2>All Ideas</h2>
  <table class="table idea-table">
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Quality</th>
      <th></th>
    </tr>
    <% @ideas.each do |idea|%>

    <tr class="<%= idea.title %>">
      <td> <%= idea.title %></td>
      <td> <%= idea.body %></td>
      <td> <%= idea.quality %></td>
      <!-- <td> <%#= link_to "Delete Entry", meal_path(meal), method: :delete, class: "btn btn-danger" %> -->
    </tr>
    <% end  %>
    </table>
  </div>
